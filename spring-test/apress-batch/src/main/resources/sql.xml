<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="batch.customer">

    <select id="selectCnt" resultType="Integer">
        SELECT COUNT(*)
        FROM customer
        WHERE customer_id = #{id}
    </select>
</mapper>