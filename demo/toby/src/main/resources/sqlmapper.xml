<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.gngsn.toby.transaction.MemberDAO">
    <insert id="insertMember" parameterType="com.gngsn.toby.transaction.Member">
            INSERT INTO test (name, email, PASSWORD)
            VALUES (#{name}, #{email}, #{password})
    </insert>

    <select id="selectMemberCnt" resultType="Integer">
            SELECT count(*) FROM test;
    </select>
</mapper>